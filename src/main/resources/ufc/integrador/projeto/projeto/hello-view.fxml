<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="285.0" prefWidth="496.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ufc.integrador.projeto.projeto.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
  <Button onAction="#onHelloButtonClick" text="Oi Miguel">
      <font>
         <Font name="Calibri" size="14.0" />
      </font></Button>

  <Label fx:id="welcomeText" text="Teste" />
   <TextField alignment="CENTER" prefHeight="26.0" prefWidth="305.0">
      <font>
         <Font name="Arial" size="14.0" />
      </font>
      <cursor>
         <Cursor fx:constant="TEXT" />
      </cursor>
      <effect>
         <Blend />
      </effect></TextField>
</VBox>
